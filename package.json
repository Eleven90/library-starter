    "dist"
  ],
  "author": "--username--",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "concurrently \"rollup -c rollup.config.ts --environment NODE_ENV:development -w\" \"tsc -p tsconfig.dev.json -w\"",
    "test": "jest --coverage --watchAll",
    "build": "concurrently \"rollup -c rollup.config.ts --environment NODE_ENV:production\" \"tsc\" \"typedoc\"",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "postinstall": "ts-node tools/init"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts}": [
      "prettier --write",
      "npm run lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "exec < /dev/tty && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "exec < /dev/tty && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
