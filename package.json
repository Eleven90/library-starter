{
  "name": "--libraryname--",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/--libraryname--.umd.js",
  "module": "dist/--libraryname--.es.js",
  "typings": "dist/types/--libraryname--.d.ts",
  "files": [
    "dist"
  ],
  "author": "--username--",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "concurrently \"rollup -c rollup.config.ts --environment NODE_ENV:development -w\" \"tsc -p tsconfig.dev.json -w\"",
    "build": "concurrently \"rollup -c rollup.config.ts --environment NODE_ENV:production\" \"tsc\"",
    "build:docs": "typedoc",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "postinstall": "ts-node tools/init"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "exec < /dev/tty && lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/",
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime-corejs3": "^7.11.2",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.2",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-config": "^26.4.2",
    "lint-staged": "^10.2.13",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.1.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.26.9",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^6.1.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {}
}
